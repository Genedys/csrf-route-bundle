{
    "name": "genedys/csrf-route-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 bundle which provides a simple way to add CSRF tokens to routes",
    "keywords": ["route", "csrf", "security"],
    "homepage": "https://github.com/Genedys/csrf-route-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Antoine",
            "email": "fabien@fantoine.fr"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/framework-bundle": "~2.1,<3.0"
    },
    "autoload": {
        "psr-0": {
            "Genedys\\CsrfRouteBundle": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "target-dir": "Genedys/CsrfRouteBundle"
}
